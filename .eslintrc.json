{
	"extends": "airbnb-base",
	"rules": {
		"arrow-spacing": "error",
		"no-use-before-define": ["error", { "functions": false, "classes": true }],
		"no-underscore-dangle": ["error", { "allowAfterThis": true, "allow": [
			"_id",
			"_deleted",
			"_tenant",
			"_ga"
		] }],
		"one-var-declaration-per-line": ["error", "initializations"],
		"camelcase": "off",
		"consistent-return": "off",
		"no-tabs": "off",
		"no-useless-escape": "warn",
		"curly": [
			"error",
			"all"
		],
		"dot-notation": "off",
		"eol-last": "off",
		"eqeqeq": [
			"error",
			"always",
			{
				"null": "ignore"
			}
		],
		"handle-callback-err": "off",
		"max-len": ["error", {
			"code": 140,
			"ignoreStrings": true,
			"ignoreComments": true
		}],
		"key-spacing": [
			"error",
			{
				"mode": "strict"
			}
		],
		"keyword-spacing": "error",
		"no-param-reassign": "warn",
		"global-require": "warn",
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"no-mixed-spaces-and-tabs": "error",
		"comma-spacing": [
			"error",
			{
				"before": false,
				"after": true
			}
		],
		"object-curly-spacing": [
			"error",
			"always"
		],
		"object-shorthand": "off",
		"one-var": "off",
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		],
		"strict": [
			"error",
			"never"
		],
		"wrap-iife": "off",
		"new-cap": [
			"error",
			{
				"capIsNew": false
			}
		],
		"no-alert": "off",
		"no-caller": "error",
		"no-bitwise": "error",
		"no-debugger": "error",
		"no-dupe-keys": "error",
		"no-empty": "error",
		"no-eval": "error",
		"no-extra-semi": "error",
		"no-irregular-whitespace": "off",
		"no-multi-spaces": "off",
		"no-new": "error",
		"no-plusplus": "off",
		"no-process-exit": "off",
		"no-redeclare": "error",
		"no-shadow": "off",
		"no-trailing-spaces": [
			"error",
			{
				"skipBlankLines": false
			}
		],
		"no-undef": "error",
		"no-unused-vars": [
			"error",
			{
				"vars": "local",
				"args": "none"
			}
		],
		"no-const-assign": "error",
		"no-unreachable": "error",
		"space-before-function-paren": [
			"error",
			{
				"anonymous": "never",
				"named": "never",
				"asyncArrow": "always"
			}
		],
		"space-infix-ops": "error"
	},
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	}
}
